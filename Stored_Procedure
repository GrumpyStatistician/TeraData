/* this only inserts values into table * taken from https://forgetcode.com/Teradata/1162-Executing-STORED-PROCEDURE */


CREATE PROCEDURE SP_Employee 
(IN name CHAR(20),
IN id INTEGER,
IN dept_no INTEGER,
OUT dname CHAR(10),
INOUT errstr VARCHAR(30))
 
BEGIN
DECLARE CONTINUE HANDLER FOR SQLSTATE VALUE '23505'
SET errstr = 'Duplicate Row.';
 
INSERT INTO Employee (EmpName, EmpNo, DeptNo )
VALUES (name, id, dept_no);
 
SELECT DeptName
INTO dname FROM Department
WHERE DeptNo = dept;
END;
